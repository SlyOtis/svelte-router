{
  "name": "sly-svelte-router",
  "version": "1.0.31",
  "type": "module",
  "description": "A spa router for svelte based on the History API",
  "keywords": [
    "svelte",
    "router",
    "spa",
    "history api",
    "slyotis",
    "sly"
  ],
  "publishConfig": {
    "branch": "sly-svelte-spa-router"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SlyOtis/svelte-router"
  },
  "homepage": "https://github.com/SlyOtis/svelte-router",
  "bugs": {
    "url": "https://github.com/SlyOtis/svelte-router/issues"
  },
  "author": "Martin Ottersland",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/sly-svelte-router.umd.js",
  "module": "./dist/sly-svelte-router.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/sly-svelte-router.es.js",
      "require": "./dist/sly-svelte-router.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ../tsconfig.json && tsc -p tsconfig.node.json",
    "bp": "pnpm build && pnpm publish --publish-branch=sly-svelte-spa-router"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@tsconfig/svelte": "^5.0.4",
    "@types/node": "^22.5.4",
    "svelte": "^4.2.18",
    "svelte-check": "^3.8.5",
    "tslib": "^2.6.3",
    "typescript": "^5.5.3",
    "vite": "^5.4.1",
    "vite-plugin-dts": "^4.2.1"
  }
}
