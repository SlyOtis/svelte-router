{
  "name": "sly-svelte-location-router",
  "version": "3.1.1",
  "type": "module",
  "description": "A simple svelte router based on location API",
  "keywords": [
    "svelte",
    "router",
    "location api",
    "slyotis",
    "sly",
    "location",
    "history api"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/SlyOtis/svelte-router"
  },
  "homepage": "https://github.com/SlyOtis/svelte-router",
  "bugs": {
    "url": "https://github.com/SlyOtis/svelte-router/issues"
  },
  "author": "Martin Ottersland",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "svelte": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build-svelte": "svelte-package",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "prepublishOnly": "npm run check && npm run build-svelte"
  },
  "devDependencies": {
    "@sveltejs/package": "^2.3.4",
    "@sveltejs/vite-plugin-svelte": "^5.1.0",
    "@tsconfig/svelte": "^5.0.4",
    "@types/node": "^22.5.4",
    "svelte": "^5.34.6",
    "svelte-check": "^4.2.1",
    "tslib": "^2.6.3",
    "typescript": "^5.5.3",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.2.1"
  },
  "dependencies": {
    "path-to-regexp": "^8.0.0"
  }
}
